file = open("9-170.txt")
proz = 0
kol = 0
for s in file:
    sp = [int(x) for x in s.split()]
    sp3 = []
    spn = []
    for i in range(len(sp)):
        if sp.count(sp[i]) == 3:
            sp3.append(sp[i])
        elif sp.count(sp[i]) == 1:
            spn.append(sp[i])
    proz = 3 * sum(spn)
    if len(sp3) == 3 and len(spn) == 3:
        if proz <= sp3[0]*sp3[1]*sp3[2]:
            kol = kol + 1
print(kol)
